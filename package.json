{
  "name": "node-beget",
  "version": "1.0.7",
  "description": "Client library for accessing Beget API",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "homepage": "https://olehcambel.github.io/node-beget",
  "scripts": {
    "start": "node ./build/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch --runInBand",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint 'src/**/*.ts'",
    "build": "tsc -p tsconfig.build.json",
    "doc": "node ./typedoc && gh-pages -m 'auto commit [ci skip]' -d docs",
    "prepush": "npm run lint && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/olehcambel/node-beget.git"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "beget",
    "api",
    "websites",
    "domains",
    "mail"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.1.5",
    "@types/qs": "^6.9.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "jest": "^25.2.6",
    "prettier": "^2.0.2",
    "ts-jest": "^25.3.0",
    "ts-node": "^8.8.1",
    "typedoc": "^0.17.3",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "got": "^10.7.0",
    "qs": "^6.9.3"
  }
}
